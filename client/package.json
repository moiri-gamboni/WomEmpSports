{
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"next\" \"yarn gql-gen:watch\"",
    "dev:no-gql-gen": "next",
    "build": "yarn gql-gen && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier . -w",
    "postinstall": "yarn next telemetry disable && cd .. && husky install client/.husky",
    "gql-gen": "graphql-codegen --config codegen.ts",
    "gql-gen:watch": "yarn gql-gen --watch"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@next/env": "^14.0.4",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "date-fns": "^3.2.0",
    "framer-motion": "^10.18.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "sanitize-html": "^2.11.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@chakra-ui/system": "^2.6.2",
    "@graphql-codegen/cli": "^5.0.1",
    "@graphql-codegen/client-preset": "^4.2.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.3.0",
    "@types/sanitize-html": "^2.9.5",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "sharp": "^0.33.2",
    "ts-node": "^10.9.2"
  },
  "packageManager": "yarn@4.0.2"
